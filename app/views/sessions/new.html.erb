<div style="row padding-top:10px;">
        <header>
          <h1><%= link_to image_tag("logo.png", :alt => "Fobitows", :size => "98x28"), root_path %></h1>
        </header> 
        <!--ここからwrapperー-->
        <div id="wrapper" class="clearfix">
            <!--ここからメイン-->
            <div id="main2">
                <section id="point">
                    <h2></h2>
                    <section class="clearfix">
                        <figure>
                            <figcaption></figcaption>
                        </figure>
                        <h3>Fobitows に Log in</h3>
                        <p>
                          
<hr><%# サインイン %>
<div class="text-center">
  <h1>Log in</h1>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: login_path) do |f| %>
      <div class="form-group">
        <%= f.label :email, 'Email' %>
        <%= f.email_field :email, :placeholder => "sample(^^)＠fobitows.com", class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :password, 'Password' %>
        <%= f.password_field :password, class: 'form-control' %>
      </div>
      <%= f.submit 'Log in', class: 'btn btn-primary btn-block' %>
    <% end %>
    <p>New user? <%= link_to 'Sign up now!', signup_path %></p>
  </div>
</div>

                        </p>
                    </section>
                </section>
            </div>
        </div>
